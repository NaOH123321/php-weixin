<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <remove name="zerg" />
                <rule name="zerg">
                    <match url="(.*)" />
                    <action type="Rewrite" url="public/index.php/{R:1}" />
                </rule>
            </rules>
        </rewrite>
        <handlers>
            <remove name="PHP" />
            <add name="PHP" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="E:\web\php-7.0.33\php-cgi.exe" resourceType="File" />
        </handlers>
    </system.webServer>
</configuration>
